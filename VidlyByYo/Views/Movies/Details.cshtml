@model VidlyByYo.Models.Movie

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>@Model.Name</h2>

<li>Genero: @Model.Genre.Name</li>
<li>Fecha de Lanzamiento: @Model.ReleaseDate.ToShortDateString()</li>
<li>Agregado el Dia: @Model.DateAdded.ToShortDateString()</li>
<li>Stock: @Model.NumberInStock</li>

